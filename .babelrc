{
  "env": {
    "commonjs": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "16.13.0"
            },
            "modules": "commonjs"
          }
        ]
      ]
    },
    "module": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "14.15"
            },
            "modules": false
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "sourceMaps": "inline"
    }
  },
  "sourceMaps": true
}
